{
  "name": "cyberneurosis",
  "publisher": "Martin Couso",
  "displayName": "Cyberneurosis",
  "description": "An over-the-top cyberpunk theme for Visual Studio Code. High tech, Low life.",
  "version": "0.1.0",
  "main": "extension.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinCouso/vscode-theme-neon-dreams.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "commands": [
      {
        "command": "cyberneurosis.implantNeonMod",
        "title": "Cyberneurosis: Implant Neon Mod"
      },
      {
        "command": "cyberneurosis.removeNeonMod",
        "title": "Cyberneurosis: Remove Neon Mod"
      }
    ],
    "configuration": {
      "title": "Cyberneurosis",
      "properties": {
        "cyberneurosis.enable": {
          "type": "boolean",
          "default": true,
          "description": "Enable Neon Mod"
        }
      }
    },
    "themes": [
      {
        "label": "Cyberneurosis",
        "uiTheme": "vs-dark",
        "path": "./themes/Cyberneurosis-color-theme.json"
      }
    ]
  },
  "activationEvents": [
    "onCommand:cyberneurosis.implantNeonMod",
    "onCommand:cyberneurosis.removeNeonMod"
  ]
}
